#include<stdio.h>
main(){
    int x=5;
    printf("Pick the menu: \n 1.Pizza\n 2.Burger\n 3.Pasta\n 4.French Fries \n 5.Sandwich\n");
    scanf("%d",&x);





    switch(x){
    case 1:printf("Food item -Pizza \n Price-Rs 279\n");
    break;
    case 2:printf("Food item -Burger \n Price-Rs 129\n");
    break;
    case 3:printf("Food item -Pasta \n Price-Rs 179 \n");
    break;
    case 4:printf("Food item -French Fries \n Price-Rs 99 \n");
    break;
    case 5:printf("Food item -Sandwich \n Price-Rs 149");
    break ;


    default :printf("NOT mentioned above");
    }



}
